               <gap reason="illegible" atLeast="8" atMost="10" unit="character"/>
               <unclear>ἔ</unclear>χ<unclear>ε</unclear>ι γέρας ἐκ τ<unclear>ῶν</unclear>
               <gap reason="lost" extent="unknown" unit="character"/><lb/> τοῦτο καὶ
                  <orig>ἐβ</orig><gap reason="illegible" quantity="1" unit="character"/>
